@Library('mylibrary@main') _
pipeline{
  agent any
  environment {
        ARTIFACTORY_CREDENTIALS = credentials('JFROG_PASSWORD')
    }
  stages{
    stage('Heading'){
      steps{
      echo 'This job will automate the CICD'
      }
    }
    stage('checkout'){
      steps{
      checkoutFile()
      }
    }
    stage('Build') {
      steps{
       buildFile()
      }
    }
stage('Publish') {
steps {
      publishFile()
    }
  }
stage('deploy') {
steps{

}
}
}
  post{
    failure{
      echo 'The build has failed'
    }
  }
}

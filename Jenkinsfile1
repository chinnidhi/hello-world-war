@Library('mylibrary-1') _
pipeline{
  agent any
  environment {
        ARTIFACTORY_CREDENTIALS = credentials('JFROG_PASSWORD')
    }
  stages{
    stage('Heading'){
      steps{
      echo 'This job will automate the CICD'
      }
    }
    stage('checkout'){
      steps{
      checkoutFile()
      }
    }
    stage('Build') {
     buildFile()
}
}
  post{
    failure{
      echo 'The build has failed'
    }
  }
}
